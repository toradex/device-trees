				vactive = <800>;
				hback-porch = <60>;
				hfront-porch = <60>;
				vback-porch = <7>;
				vfront-porch = <7>;
				hsync-len = <40>;
				vsync-len = <9>;
				hsync-active = <0>;
				vsync-active = <0>;
