	#address-cells = <1>;
	#size-cells = <0>;
	/delete-property/virtual-channel;
};

&mipi0_dsi_host {
	pwr-delay = <10>;

	ports {
		#address-cells = <1>;
		#size-cells = <0>;
		port@1 {
			reg = <1>;
			mipi0_dsi_host_out: endpoint {
				remote-endpoint = <&lt8912_in>;
			};
		};
	};
};

&mipi1_dsi_host {
