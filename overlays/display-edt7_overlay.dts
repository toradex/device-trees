// EDT7 display with a resolution of 800x480 pixel

/dts-v1/;
/plugin/;

/ {
	compatible = "toradex,colibri-imx7d-emmc","toradex,apalis_imx6q", "toradex,colibri_imx6dl";
};

&{/panel-dpi} {
	status = "okay";

	width-mm = <155>;
	height-mm = <91>;

	panel-timing {
		clock-frequency = <33230769>;
		hactive = <800>;
		vactive = <480>;
		hsync-len = <128>;
		hfront-porch = <40>;
		hback-porch = <88>;
		vsync-len = <1>;
		vfront-porch = <10>;
		vback-porch = <33>;
	};
};

